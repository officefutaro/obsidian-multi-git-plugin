{
  "name": "obsidian-multi-git-plugin",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "dev": "esbuild src/main.ts --bundle --external:obsidian --outdir=. --target=es2016 --format=cjs --sourcemap --watch --platform=node",
    "build": "esbuild src/main.ts --bundle --external:obsidian --outdir=. --target=es2016 --format=cjs --platform=node",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/main.test.ts",
    "test:integration": "jest tests/git-operations.test.ts",
    "test:all": "npm run build && npm run test:setup && npm test",
    "test:setup": "node -e \"require('child_process').execSync('scripts/setup-test-env.bat', {stdio: 'inherit'})\"",
    "test:ai": "node ../scripts/claude-test-runner.js",
    "test:ai-watch": "npm run test:ai && echo 'Watching for changes...' && npm run dev",
    "test:ai-schedule": "node ../scripts/ai-test-scheduler.js",
    "test:ai-dashboard": "node ../scripts/ai-test-dashboard.js",
    "test:ai-dashboard-watch": "node ../scripts/ai-test-dashboard.js watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "Obsidian plugin for managing multiple Git repositories",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "esbuild": "^0.25.11",
    "jest": "^30.2.0",
    "obsidian": "^1.10.0",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
